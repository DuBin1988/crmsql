<?xml version="1.0"  encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="test">
	<!--街道管理-->
	<class entity-name="t_road" table="t_road">
	   <id name="id" type="int">
			<generator class="identity"></generator>
		</id>
		<!-- 街道名称 -->
	 	<property name="f_road" type="string"/>
		<!-- 街道范围 -->
	 	<property name="f_roadrange" type="string"/>
	 	<!-- 操作日期 -->
	 	<property name="f_date" type="date"/>
		<!--操作员 -->
		<property name="f_operator" type="string"/>
		<!--网点-->
		<property name="f_network" type="string"/>
		<!--分公司-->
		<property name="f_branch" type="string"/>
		<!--组织机构字符串拼接-->
		<property name="f_orgstr" type="string"/>
	</class>
</hibernate-mapping>