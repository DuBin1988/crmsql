<?xml version="1.0"  encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="test">
	<!--修改限购气量-->
	<class entity-name="t_xiangoujilu" table="t_xiangoujilu">
	    <id name="id" type="int">
			<generator class="identity"></generator>
		</id>   
		<!--用户编号-->
		<property name="f_userid" type="string"/>
		<!-- 限购气量修改 -->
		<property name="f_limitbuygas" type="double"/>
		<!--  修改原因 -->
		<property name="f_mnate" type="string"/>
		<!-- 类型 -->
		<property name="f_gasproperties" type="string"/>
		<!-- 民用上限 -->
		<property name="myxgmax" type="string"/>
		<!-- 非民用上限 -->
		<property name="gyxgmax" type="string"/>
		<!-- 混合型上限 -->
		<property name="hemax" type="string"/>
		<!-- 部门 -->
		<property name="f_department" type="string"/>
		<!-- 分公司 -->
		<property name="f_fengongsinum" type="string"/>
		<!-- 分公司编号 -->
		<property name="f_filiale" type="string"/>
		<!-- 操作员 -->
		<property name="f_operator" type="string"/>
		<!-- 操作日期 -->
		<property name="f_date" type="date"/>
		
	</class>
</hibernate-mapping>